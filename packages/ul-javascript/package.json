{
  "name": "ul-javascript",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./login-id": {
      "import": "./dist/esm/screens/login-id.js",
      "require": "./dist/cjs/screens/login-id.js"
    },
    "./login-password": {
      "import": "./dist/esm/screens/login-password.js",
      "require": "./dist/cjs/screens/login-password.js"
    },
    "./signup-id": {
      "import": "./dist/esm/screens/signup-id.js",
      "require": "./dist/cjs/screens/signup-id.js"
    },
    "./signup-password": {
      "import": "./dist/esm/screens/signup-password.js",
      "require": "./dist/cjs/screens/signup-password.js"
    },
    "./login-passwordless-email-code": {
      "import": "./dist/esm/screens/login-passwordless-email-code.js",
      "require": "./dist/cjs/screens/login-passwordless-email-code.js"
    },
    "./login-passwordless-email-link": {
      "import": "./dist/esm/screens/login-passwordless-email-link.js",
      "require": "./dist/cjs/screens/login-passwordless-email-link.js"
    },
    "./login-passwordless-sms-otp": {
      "import": "./dist/esm/screens/login-passwordless-sms-otp.js",
      "require": "./dist/cjs/screens/login-passwordless-sms-otp.js"
    },
    "./consent": {
      "import": "./dist/esm/screens/consent.js",
      "require": "./dist/cjs/screens/consent.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && rollup -c rollup.config.js --bundleConfigAsCjs",
    "test": "jest",
    "test:e2e": "cypress open",
    "docs": "typedoc --options typedoc.json",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "rollup": "^4.24.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.8",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
