{
  "name": "acul-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "screen:standard": "node scripts/standard-mode.js",
    "screen:advanced": "node scripts/advanced-mode.js",
    "prenormalize": "chmod +x scripts/build/normalize-case.js",
    "normalize": "node scripts/build/normalize-case.js",
    "prebuild": "npm run normalize && rimraf dist && rimraf tsconfig.tsbuildinfo",
    "build": "tsc -b --force && vite build",
    "serve": "http-server dist -p 3032 --cors --silent",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@auth0/auth0-acul-js": "^0.1.0-beta.2",
    "@tailwindcss/vite": "^4.0.11",
    "axios": "^1.8.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "dotenv": "^16.4.7",
    "execa": "^8.0.1",
    "http-server": "^14.1.1",
    "ora": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fs-extra": "^11.3.0",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4.1.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
