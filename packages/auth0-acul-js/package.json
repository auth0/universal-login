{
  "name": "@auth0/auth0-acul-js",
  "version": "1.0.0-alpha.4",
  "type": "module",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/src/index.d.ts"
    },
    "./types": {
      "types": "./dist/types/interfaces/export/index.d.ts"
    },
    "./*": {
      "import": "./dist/screens/*/index.js",
      "types": "./dist/types/src/screens/*/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm run clean && npm run lint && npm run test && npm run docs && rollup -c rollup.config.mjs",
    "build:local": "NODE_ENV=production rollup -c rollup.config.mjs",
    "dev": "NODE_ENV=development rollup -c rollup.config.mjs -w",
    "dev:prod": "NODE_ENV=production rollup -c rollup.config.mjs -w",
    "test": "jest --verbose tests/unit/**/* --coverage",
    "test:e2e": "cypress open",
    "docs": "typedoc --options typedoc.js",
    "lint": "npm run lint:interfaces && npm run lint:src",
    "lint:fix": "npm run lint:src:fix && npm run lint:interfaces:fix",
    "lint:interfaces": "eslint 'interfaces/**/*.ts' --no-ignore",
    "lint:src": "eslint 'src/**/*.ts' --no-ignore",
    "lint:src:fix": "eslint 'src/**/*.{js,ts}' --no-ignore --fix",
    "lint:interfaces:fix": "eslint 'interfaces/**/*.ts' --no-ignore --fix",
    "lint:watch": "chokidar 'src/**/*.{js,ts}' 'interfaces/**/*.ts' -c 'npm run lint'",
    "format:src": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
    "format:interfaces": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
    "format": "npm run format:src && npm run format:interfaces"
  },
  "author": "Auth0",
  "license": "MIT",
  "description": "The Auth0 ACUL JS SDK enables you to work with Advanced Customization for Universal Login.",
  "repository": {
    "type": "git",
    "url": "https://github.com/auth0/universal-login/tree/master/packages/auth0-acul-js"
  },
  "homepage": "https://github.com/auth0/universal-login/tree/master/packages/auth0-acul-js#readme",
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "auth0",
    "ACUL"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
